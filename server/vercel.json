{
  "version": 2,
  "builds": [
    { "src": "./index.ts", "use": "@vercel/node" },
    { "src": "data/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/data/(.*)", "dest": "/data/$1" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
